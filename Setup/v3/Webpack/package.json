{
  "name": "vue-hmr-eslint-webpack-babel",
  "description": "Using Vue with Hot Module Replacement, ESLint, Webpack & Babel",
  "version": "1.0.0",
  "author": "Rick Beerendonk",
  "license": "EUPL-1.2",
  "devDependencies": {
    "@babel/cli": "7.12.10",
    "@babel/core": "7.12.10",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.12.11",
    "@vue/compiler-sfc": "3.0.5",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "copy-webpack-plugin": "6.4.1",
    "css-loader": "5.0.1",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-vue": "7.5.0",
    "html-webpack-plugin": "4.5.1",
    "style-loader": "2.0.0",
    "vue-eslint-parser": "7.4.1",
    "vue-loader": "16.1.2",
    "vue-template-compiler": "2.6.12",
    "webpack": "4.46.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.2"
  },
  "dependencies": {
    "vue": "3.0.5"
  },
  "scripts": {
    "build": "npm run clean && webpack --progress",
    "clean": "rimraf dist",
    "eslint": "eslint . --ext .js,.jsx,.vue --cache",
    "eslint-fix": "eslint . --ext .js,.jsx,.vue --cache --fix",
    "serve": "webpack-dev-server --hot --open",
    "serve-debug": "webpack-dev-server"
  }
}
