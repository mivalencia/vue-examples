{
  "name": "vue-examples",
  "version": "1.0.0",
  "description": "Examples for [Vue.js](https://vuejs.org).",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickbeerendonk/react-examples.git"
  },
  "author": "Rick Beerendonk",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/rickbeerendonk/react-examples/issues"
  },
  "homepage": "https://github.com/rickbeerendonk/react-examples#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate",
      "pre-push": "npm run validate"
    }
  },
  "scripts": {
    "clean": "rimraf node_modules",
    "eslint": "eslint . --ext .js,.jsx --cache",
    "eslint-fix": "eslint . --ext .js,.jsx --cache --fix",
    "open": "open",
    "openserver": "node \"./Util 01 - Custom Server/server.js\" 8090",
    "prettier": "prettier --list-different '**/*.{json,js}'",
    "prettier-fix": "prettier --write '**/*.{json,js}'",
    "validate": "npm run eslint && npm run prettier"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-optional-catch-binding": "^7.0.0-alpha.16",
    "css-loader": "^1.0.1",
    "eslint": "^5.10.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "html-webpack-plugin": "next",
    "husky": "^1.3.0",
    "prettier": "^1.15.3",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.21",
    "webpack": "next",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.12"
  },
  "dependencies": {
    "http-server": "^0.11.1",
    "http-vue-loader": "^1.3.5",
    "vue": "^2.5.21"
  }
}
