{
  "name": "vue-examples",
  "version": "1.0.0",
  "description": "Examples for [Vue.js](https://vuejs.org).",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickbeerendonk/vue-examples.git"
  },
  "author": "Rick Beerendonk",
  "license": "EUPL-1.2",
  "bugs": {
    "url": "https://github.com/rickbeerendonk/vue-examples/issues"
  },
  "homepage": "https://github.com/rickbeerendonk/vue-examples#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "scripts": {
    "clean": "rimraf node_modules",
    "eslint": "eslint . --ext .js,.jsx,.vue --cache",
    "eslint-fix": "eslint . --ext .js,.jsx,.vue --cache --fix",
    "open": "open",
    "openserver": "node \"./Util - 01 - Custom Server/server.js\" 9002",
    "prettier": "prettier --list-different '**/*.{json,js,jsx,html,md,vue}'",
    "prettier-fix": "prettier --write '**/*.{json,js,jsx,html,md,vue}'",
    "validate": "npm run eslint && npm run prettier",
    "start": "npm run openserver -- \".\""
  },
  "devDependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/polyfill": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "copy-webpack-plugin": "6.1.1",
    "css-loader": "4.3.0",
    "eslint": "7.9.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-vue": "6.2.2",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.0",
    "prettier": "2.1.2",
    "vue-eslint-parser": "7.1.0",
    "vue-loader": "15.9.3",
    "vue-router": "3.4.4",
    "vue-template-compiler": "2.6.12",
    "vuex": "3.5.1",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "http-vue-loader": "1.4.2",
    "serve": "11.3.2",
    "vue": "2.6.12",
    "vue-next": "npm:vue@3.0.0"
  }
}
