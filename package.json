{
  "name": "vue-examples",
  "version": "1.0.0",
  "description": "Examples for [Vue.js](https://vuejs.org).",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickbeerendonk/vue-examples.git"
  },
  "author": "Rick Beerendonk",
  "license": "EUPL-1.2",
  "bugs": {
    "url": "https://github.com/rickbeerendonk/vue-examples/issues"
  },
  "homepage": "https://github.com/rickbeerendonk/vue-examples#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "scripts": {
    "clean": "rimraf node_modules",
    "eslint": "eslint . --ext .js,.jsx,.vue --cache",
    "eslint-fix": "eslint . --ext .js,.jsx,.vue --cache --fix",
    "open": "open",
    "openserver": "node \"./Util - 01 - Custom Server/server.js\" 9002",
    "prettier": "prettier --list-different '**/*.{json,js,html,md,vue}'",
    "prettier-fix": "prettier --write '**/*.{json,js,html,md,vue}'",
    "validate": "npm run eslint && npm run prettier",
    "start": "npm run openserver -- \".\""
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.4",
    "@babel/polyfill": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.0.6",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.4.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-vue": "6.2.1",
    "html-webpack-plugin": "4.0.0-beta.11",
    "husky": "4.2.3",
    "prettier": "1.19.1",
    "vue-eslint-parser": "7.0.0",
    "vue-loader": "15.9.0",
    "vue-router": "3.1.6",
    "vue-template-compiler": "2.6.11",
    "vuex": "3.1.2",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  },
  "dependencies": {
    "http-vue-loader": "1.4.2",
    "serve": "11.3.0",
    "vue": "2.6.11",
    "vue-next": "npm:vue@next"
  }
}
